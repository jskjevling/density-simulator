package {	import flash.display.Sprite;	import LineText;		public class HeightMm extends Sprite {		private var textOne:LineText;		private var val:String;		private var deqX:Number;		private var deqY:Number;		private var txtRotation:Number;				public function HeightMm(val:String, deqX:Number, deqY:Number, txtRotation:Number) {			this.val = val;			this.deqX = deqX;			this.deqY = deqY;			this.txtRotation = txtRotation;			init();		}				private function init():void {			textOne = new LineText(val, 0x333333, 0, 0, txtRotation);			addChild(textOne);			this.x = this.deqX;			this.y = this.deqY;		}	}}