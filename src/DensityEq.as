package {	import flash.display.Sprite;	import LineText;		public class DensityEq extends Sprite {		private var textOne:LineText;		private var textTwo:LineText;		private var textThree:LineText;		private var deqX:Number;		private var deqY:Number;				public function DensityEq(deqX:Number, deqY:Number) {			this.deqX = deqX;			this.deqY = deqY;			init();		}				private function init():void {			textOne = new LineText("ρ = —", 0x333333, 10, 25, 0);			addChild(textOne);			textTwo = new LineText("m", 0x333333, 32, 15, 0);			addChild(textTwo);			textThree = new LineText("V", 0x333333, 32, 37, 0);			addChild(textThree);			this.x = this.deqX;			this.y = this.deqY;		}	}}